{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10"
  },
  "installCommand": "pip install --upgrade pip setuptools && pip install -r requirements.txt",
  "buildCommand": "echo 'Build step complete!'"
}
